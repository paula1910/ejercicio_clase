function setup(){
    ;
    
    
    createCanvas(200,200);//200 ancho x 200 alto
    background(225); // Rango de grises

    

}


function draw(){ 
  beginShape();
  vertex(100,30);
  vertex(50,100);
  vertex(150,60);
  vertex(80,40);
  vertex(100,30);
  //agrega mas vertices
  endShape(CLOSE);
   
}
